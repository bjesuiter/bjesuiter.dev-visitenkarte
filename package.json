{
  "name": "visitenkarte",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "start": "bunx --bun astro dev",
    "build": "bunx --bun astro check && bunx --bun astro build",
    "upgrade": "bunx --bun @astrojs/upgrade",
    "preview-deno": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "preview": "bunx --bun astro preview",
    "astro": "astro",
    "deploy": "surge ./dist"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^6.0.0",
    "@deno/astro-adapter": "^0.2.0",
    "@iconify-json/bxl": "^1.2.2",
    "@iconify-json/ion": "^1.2.2",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.23",
    "astro": "^5.2.3",
    "astro-font": "^1.0.0",
    "astro-icon": "^1.1.5",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "surge": "^0.24.6",
    "tailwindcss-motion": "^1.0.1"
  }
}
